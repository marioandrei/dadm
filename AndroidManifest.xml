            android:name=".Initial"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Session"
            android:label="" >
            <intent-filter>
                <action android:name="com.es.uam.eps.dadm.mario_pantoja.SESSION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name=".Paint"
            android:label="" >
            <intent-filter>
                <action android:name="com.es.uam.eps.dadm.mario_pantoja.PAINT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>